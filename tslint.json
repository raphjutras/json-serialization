{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "variable-name": false,
    "no-console": false,
    "ban-types": false,
    "prefer-const": false,
    "unified-signatures": false
  }
}
