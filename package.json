{
  "name": "@peerlancers/json-serialization",
  "description": "A typescript library for deserializing JSON to object and serializing object to JSON.",
  "version": "0.0.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Arrian Pascual",
  "license": "MIT",
  "keywords": [
    "object-mapper",
    "json-serialization",
    "typescript mapper",
    "object mapper",
    "json serialization",
    "json-serialization",
    "typescript serialization",
    "serializer"
  ],
  "homepage": "https://github.com/apascual-pl/json-serialization#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/apascual-pl/json-serialization"
  },
  "bugs": {
    "type": "git",
    "url": "https://github.com/apascual-pl/json-serialization/issues"
  },
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "serialization:build": "tsc",
    "serialization:publish": "npm run serialization:build && npm run test && npm publish --access public",
    "execute": "npm run build && cd dist && node sample.js"
  },
  "typescript": {
    "definition": "index.d.ts"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/node": "^12.7.4",
    "ts-node": "^8.3.0",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "typescript": "~3.4.3"
  }
}